{
    "app": {
        "name": "myAppName",
        "url": "https://myapp.com",
        "allowed_origins": ["https://myapp.com"],
        "api_keys": ["xxxxxxxxxxxx"]
    },
    "db": {
        "postgres_url": "xxxxx" 
    },
    "jwt": {
        "secret": "xxxxxx",
        "access_token_expiration_minutes": 15,
        "refresh_token_expiration_days": 30
    },
    "auth": {
        "providers": {
            "github": {
                "enabled": true,
                "app_name": "myAppName",
                "client_id": "xxxxxx",
                "client_secret": "xxxxxx"
            }
        }
    },
    "cookie": {
        "domain": "https://myapp.com",
        "secure": true,
        "http_only": true,
        "same_site": 1,
        "path": "/"
    },
    "user": {
        "roles": ["platform_admin", "user"]
    }
}
